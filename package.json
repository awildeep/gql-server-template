{
   "name": "typegraphql",
   "version": "1.0.0",
   "main": "index.js",
   "license": "MIT",
   "scripts": {
      "start": "yarn run clear:dist && ts-node-dev --respawn src/index.ts",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
      "fixtures": "ts-node src/LoadFixtures.ts",
      "clear:dist": "rm -Rf dist",
      "test:db:setup": "ts-node src/TestUtils/Setup.ts",
      "test:db:fixtures": "ts-node src/TestUtils/TestLoadFixtures.ts",
      "test": "yarn run clear:dist && yarn run test:db:setup && yarn run test:db:fixtures && jest --detectOpenHandles",
      "pretty": "prettier --check \"./**/*.{js,ts,json}\"",
      "pretty:fix": "prettier --write \"./**/*.{js,ts,json}\"",
      "lint": "eslint . --ext=js,ts,json",
      "lint:fix": "eslint . --ext=js,ts,json --fix"
   },
   "lint-staged": {
      "src/**/*.{js,ts,json}": [
         "yarn run pretty",
         "yarn run lint --max-warnings=0"
      ]
   },
   "husky": {
      "hooks": {
         "pre-commit": "lint-staged"
      }
   },
   "dependencies": {
      "apollo-server-express": "^2.14.2",
      "bcryptjs": "^2.4.3",
      "class-validator": "^0.12.2",
      "cors": "^2.8.5",
      "date-fns": "^2.6.0",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "express-jwt": "^6.0.0",
      "express-session": "^1.17.0",
      "graphql": "^14.1.1",
      "jsonwebtoken": "^8.5.1",
      "nodemailer": "^6.4.16",
      "pg": "^8.2.1",
      "reflect-metadata": "^0.1.13",
      "type-graphql": "^0.17.6",
      "typeorm": "0.2.25",
      "uuid": "^8.2.0",
      "verbal-expressions": "^1.0.2"
   },
   "devDependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/connect-redis": "^0.0.14",
      "@types/cors": "^2.8.6",
      "@types/date-fns": "^2.6.0",
      "@types/express": "^4.17.1",
      "@types/express-jwt": "^0.0.42",
      "@types/express-session": "^1.15.15",
      "@types/faker": "^4.1.7",
      "@types/graphql": "^14.5.0",
      "@types/ioredis": "^4.0.18",
      "@types/jest": "^26.0.4",
      "@types/jsonwebtoken": "^8.3.5",
      "@types/node": "^8.0.29",
      "@types/nodemailer": "^6.2.2",
      "@types/reflect-metadata": "^0.1.0",
      "@types/uuid": "^8.0.0",
      "@typescript-eslint/eslint-plugin": "^3.6.0",
      "@typescript-eslint/parser": "^3.6.0",
      "eslint": "^7.4.0",
      "eslint-config-prettier": "^6.11.0",
      "eslint-plugin-prettier": "^3.1.4",
      "faker": "^4.1.0",
      "husky": "^4.2.5",
      "jest": "^26.1.0",
      "lint-staged": "^10.2.11",
      "prettier": "^2.0.5",
      "ts-jest": "^26.1.1",
      "ts-node": "8.10.2",
      "ts-node-dev": "^1.0.0-pre.43",
      "tsconfig-paths": "^3.9.0",
      "typeorm-fixtures-cli": "^1.3.7",
      "typescript": "^3.7.2"
   }
}
